<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <meta name="viewport" width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0>
    <title>Sherping Algorithms</title>

    <!-- Add styles -->
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/css/bootstrap.min.css" integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm" crossorigin="anonymous">
    <link rel="stylesheet" href="css/styles.css">
    <link rel="stylesheet" href="css/testStyles.css">
    <!-- Add Google font -->
    <link href="https://fonts.googleapis.com/css?family=Orbitron" rel="stylesheet">
    <!-- Add favicon -->
    <link rel="icon" type="image/png" href="css/images/favicon.ico">

    <!-- Add p5.js scripts -->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/p5.js/0.7.1/p5.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/p5.js/0.7.1/addons/p5.dom.min.js"></script>
    <!-- <script src="https://cdnjs.cloudflare.com/ajax/libs/p5.js/0.7.1/addons/p5.sound.min.js"></script> -->
    <script	src="https://cdnjs.cloudflare.com/ajax/libs/chance/1.0.18/chance.min.js"></script>

    <!-- Add my .js scripts -->
    <script src="js/home/manageControls.js"></script>
    <script src="js/home/manageEnv.js"></script>
    <script src="js/home/environmentHome.js"></script>
    <script src="js/UAVAgent.js"></script>
    <script src="js/shepherdManual.js"></script>
    <script src="js/singleGPSUAV.js"></script>
    <script src="js/multiGPS.js"></script>
    <script src="js/multiOracleShepherd.js"></script>
    <script src="js/novelty.js"></script>
    <script src="js/obstacle.js"></script>
    <script src="js/gate.js"></script>
    <script src="js/oracle.js"></script>
    <script src="js/oracleShepherd.js"></script>
    <script src="js/animalAgent.js"></script>
    <script src="js/home/tameAnimal.js"></script>
    <script src="js/home/flightyAnimal.js"></script>
    <script src="js/home/sketchHome.js"></script>

  </head>
  <body>
    <!-- Use header partial -->
    {{> header}}

    <!-- Canvas div -->
    <div class="container-fluid" align:'center'>
      <div class="row" id="simRow">
        <!-- Buttons div -->
        <div class="col-lg-2" id="startingParamsDiv">
          <div>
            <div>
              <!-- Buttons to initialize UAV's/ start simulation-->
              <h6><strong><u>Run It...</u></strong></h6>
              <!-- Intilize UAV Buttons -->
              <div id="singleGPSBtn">
              </div>
              <div id="singleOracleBtn">
              </div>
              <div id="multiGPSBtn">
              </div>
              <div id="multiOracleBtn">
              </div>
              <br><br>

              <!-- Radio buttons to start with differnt starting parameters -->
              <h6><strong><u>Select Env</u></strong></h6>
              <div id="envRadio">   <!-- Radio Button to select environment -->
              </div>
              <h6><strong><u>Select Path</u></strong></h6>
              <div class="path label" id="pathRadio"> <!-- Reactiive Avoidance vs Path Planning -->
              </div>
              <br>
              <h6><strong><u>Animals</u></strong></h6>
              <div id="anNumRadio"> <!-- Number of Animals -->
              </div>
              <h6><strong><u>Animal Type</u></strong></h6>
              <div class="path label" id="anTypeRadio"> <!-- Tame vs Flighty animals -->
              </div>
              <br>
              <h6><strong><u>Number of tests</u></strong></h6>
              <div class="hmmm label" id="testNumRadio"> <!-- Number of tests -->
              </div>
              <br>
              <!-- Pause/reset buttons -->
              <div id="pauseBtn">
              </div>
              <div id="resetBtn">
              </div>
            </div>
          </div>
          <br>

        </div> <!-- Close moreControls col -->
        <div class="col-lg-10">
          <div id="myCanvas"></div>   <!-- Canvas -->
        </div>
      </div> <!-- Close Simrow -->

      <!-- Info Row Titles -->
      <div class="row simHeader">
          <div class="col-lg-2 borderRight infoHeader" >
            <h5><b>Env Info</b></h5>
          </div>
          <div class="col-lg-2 borderRight infoHeader">
            <h5><b>Herd Info</b></h5>
          </div>
          <div class="col-lg-2 borderRight infoHeader">
            <h5><b>Shepherd</b></h5>
          </div>
          <div class="col-lg-2 borderRight infoHeader">
            <h5><b>Oracle UAV</b></h5>
          </div>
          <div class="col-lg-2 borderRight infoHeader">
            <h5><b>Animal Display</b></h5>
          </div>
          <div class="col-lg-2 infoHeader">
            <h5><b>UAV Display</b></h5>
          </div>
      </div>

      <!-- Simulation Info display div -->
      <div class="row" id="controls">
        <div class="col-lg-2 con" id='control1'>
          <div id="envInfoDiv">
          </div>
        </div>

        <!-- Animal agent info display div -->
        <div class="col-lg-2 con" id="control2" align="center">
          <div id="animalInfoDiv">
          </div>
        </div>

        <!-- Gps UAV display div -->
        <div class="col-lg-2 con" id="control3">
          <br>
          <div id="gpsInfoDiv">
          </div>
        </div>

        <!-- Oracle Info display div -->
        <div class="col-lg-2 con" id="control4">
          <br>
          <div id="oracleInfoDiv">
          </div>
        </div>

        <!-- Animal Canvas Display Sliders Div -->
        <div class="col-lg-2 con" id="control5">
          <div id="zoneDiv">
          </div>
        </div>

        <!-- UAV Canvas Display Sliders Div -->
        <div class="col-lg-2 con" id="control6">
          <div id="uavStuff">
          </div>
          <div id="oracleStuff">
          </div>
        </div>
      </div> <!-- Close controls row -->

      <!-- Test Results Title Row -->
      <div class="row simHeader">
          <div class="col-lg-2 borderRight infoHeader">
            <h5><b>Test 1-4</b></h5>
          </div>
          <div class="col-lg-2 borderRight infoHeader">
            <h5><b>Test 5-8</b></h5>
          </div>
          <div class="col-lg-2 borderRight infoHeader">
            <h5><b>Test 9-12</b></h5>
          </div>
          <div class="col-lg-2 borderRight infoHeader">
            <h5><b>Test 13-16</b></h5>
          </div>
          <div class="col-lg-2 borderRight infoHeader">
            <h5><b>16-20</b></h5>
          </div>
          <div class="col-lg-2 infoHeader">
            <h5><b>Averages</b></h5>
          </div>
      </div>

      <!-- Test Results Print Out Div -->
      <div class="row" id="controlsRow2">
        <div class="col-lg-2 con2" id="test0">
          <br>
        </div>

        <div class="col-lg-2 con2" id="test1">
          <br>
        </div>
        <div class="col-lg-2 con2" id="test2">
          <br>
        </div>
        <div class="col-lg-2 con2" id="test3">
          <br>
        </div>
        <div class="col-lg-2 con2" id="test4">
          <br>
        </div>
        <div class="col-lg-2 con2" id="avgTestsDiv">
          <br>
        </div>
      </div>

      <!-- Chart Title Div -->
      <div class="row simHeader">
          <div class="col-lg-1 borderRight infoHeader">
          </div>
          <div class="col-lg-10 borderRight infoHeader sideChart">
            <h5><b>Charts</b></h5>
          </div>
          <div class="col-lg-1 borderRight infoHeader">
          </div>
      </div>

      <!-- Chart Display Div -->
      <div class="row" id="chartsRow">
        <div class="col-lg-1 borderRight infoHeader sideChart">
        </div>
        <div class="col-lg-10 con2" id="chartDiv">
          <br>
          <canvas id="myChart"></canvas>
        </div>
        <div class="col-lg-1 borderRight infoHeader sideChart">
        </div>
      </div>

      <!-- Empty Bottom Border Div -->
      <div class="row simHeader">
          <div class="col-lg-1 borderRight infoHeader">
          </div>
          <div class="col-lg-10 borderRight infoHeader sideChart">
            <h5><b>...</b></h5>
          </div>
          <div class="col-lg-1 borderRight infoHeader">
          </div>
      </div>
    </div> <!-- Close container -->

    <!-- More scripts -->
    <script src="https://cdn.jsdelivr.net/npm/chart.js@2.8.0"></script>
    <script src="https://cdn.jsdelivr.net/npm/chartjs-plugin-datalabels@0.5.0"></script>
    <script src="https://code.jquery.com/jquery-3.2.1.slim.min.js" integrity="sha384-KJ3o2DKtIkvYIK3UENzmM7KCkRr/rE9/Qpg6aAZGJwFDMVNA/GpGFF93hXpG5KkN" crossorigin="anonymous"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.12.9/umd/popper.min.js" integrity="sha384-ApNbgh9B+Y1QKtv3Rn7W3mgPxhU9K/ScQsAP7hUibX39j7fakFPskvXusvfa0b4Q" crossorigin="anonymous"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/js/bootstrap.min.js" integrity="sha384-JZR6Spejh4U02d8jOt6vLEHfe/JQGiRRSQQxSfFWpi1MquVdAyjUar5+76PVCmYl" crossorigin="anonymous"></script>

{{> footer}} <!--Use footer partial -->
